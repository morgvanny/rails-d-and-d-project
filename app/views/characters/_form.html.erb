<%= form_for character do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :level %>
  <%= f.number_field :level %>
  <%= f.label :race %>
  RADIO
  <%= f.label :character_class %>
  RADIO
  <%= f.label :alignment %>
  RADIO
  <%= f.label :strength %>
  <%= f.number_field :strength %>
  <%= f.label :dexterity %>
  <%= f.number_field :dexterity %>
  <%= f.label :constitution %>
  <%= f.number_field :constitution %>
  <%= f.label :intelligence %>
  <%= f.number_field :intelligence %>
  <%= f.label :wisdom %>
  <%= f.number_field :wisdom %>
  <%= f.label :charisma %>
  <%= f.number_field :charisma %>
  <%= f.label :hp %>
  <%= f.number_field :hp %>
  <%= f.label :ac %>
  <%= f.number_field :ac %>
  <%= f.label :initiative %>
  <%= f.number_field :initiative %>
  <%= f.label :speed %>
  <%= f.number_field :speed %>
  <%= f.label :notes %>
  <%= f.text_field :notes %>
  <%= f.submit 'save' %>
<% end %>
